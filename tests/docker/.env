CLUSTER_SIZE=1
NODE_INDEX
DELTA_PLUGINS=/opt/docker/plugins/
DELTA_EXTERNAL_CONF=/config/delta-postgres.conf
# Caches could cause issue to integration tests
ENABLE_CACHES=false
# Some of the plugins do not scale well with a high
# number of projects and cause problems to benchmarks
ENABLE_OPTIONAL_PLUGINS=true
OTEL_SDK_DISABLED=true
PARTITION_STRATEGY=hash
